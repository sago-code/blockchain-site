{
  "name": "blockchain-site",
  "private": true,
  "version": "1.0.0",
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "concurrently -n FRONTEND,BACKEND -c green,cyan \"npm:dev --workspace=frontend\" \"npm:dev --workspace=backend\"",
    "build": "npm run build -w frontend",
    "serve:prod": "npm run start -w backend"
  },
  "devDependencies": {
    "concurrently": "^9.0.0"
  },
  "dependencies": {
    "swiper": "^12.0.2"
  }
}
